		"@types/babel__traverse": "^7.28.0",
		"@types/node": "latest",
		"chalk": "^5.6.2",
		"esbuild": "^0.25.10",
		"rollup": "^4.53.3",
		"rollup-plugin-esbuild": "^6.2.1",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3",
		"vite": "^6.4.1",
		"vitest": "^3.2.4"
	},
	"peerDependencies": {
		"@babel/parser": "^7.26.3",
		"@babel/traverse": "^7.26.4",
		"@babel/types": "^7.26.3"
